<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/product_list">

    <fragment
        android:id="@+id/product_list"
        android:name="jose.gonzalez.mobiquitysample.view.ProductsListFragment"
        android:label="ProductsListFragment" >
        <action
            android:id="@+id/action_product_list_to_product_detail"
            app:destination="@id/product_detail" />
    </fragment>

    <fragment
        android:id="@+id/product_detail"
        android:name="jose.gonzalez.mobiquitysample.view.ProductDetailFragment"
        android:label="ProductDetailFragment">
    </fragment>

</navigation>